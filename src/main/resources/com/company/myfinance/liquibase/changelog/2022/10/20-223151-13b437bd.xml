<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="myFinance">
        <dropForeignKeyConstraint baseTableName="BANK_ACCOUNT" constraintName="FK_BANK_ACCOUNT_ON_CLIENT"/>
    </changeSet>
    <changeSet id="2" author="myFinance">
        <dropForeignKeyConstraint baseTableName="TRANSACTION_" constraintName="FK_TRANSACTION__ON_FROM_ACC"/>
    </changeSet>
    <changeSet id="3" author="myFinance">
        <dropForeignKeyConstraint baseTableName="TRANSACTION_" constraintName="FK_TRANSACTION__ON_TO_ACC"/>
    </changeSet>
    <changeSet id="4" author="myFinance">
        <dropForeignKeyConstraint baseTableName="TYPE_" constraintName="FK_TYPE__ON_CLIENT"/>
    </changeSet>
    <changeSet id="5" author="myFinance">
        <dropForeignKeyConstraint baseTableName="TYPE_TRANSACTION_LINK" constraintName="FK_TYPTRA_ON_TRANSACTION"/>
    </changeSet>
    <changeSet id="6" author="myFinance">
        <dropForeignKeyConstraint baseTableName="TYPE_TRANSACTION_LINK" constraintName="FK_TYPTRA_ON_TYPE"/>
    </changeSet>
    <changeSet id="7" author="myFinance">
        <dropTable cascadeConstraints="true" tableName="BANK_ACCOUNT"/>
    </changeSet>
    <changeSet id="8" author="myFinance">
        <dropTable cascadeConstraints="true" tableName="TRANSACTION_"/>
    </changeSet>
    <changeSet id="9" author="myFinance">
        <dropTable cascadeConstraints="true" tableName="TYPE_"/>
    </changeSet>
    <changeSet id="10" author="myFinance">
        <dropTable cascadeConstraints="true" tableName="TYPE_TRANSACTION_LINK"/>
    </changeSet>
    <changeSet id="11" author="myFinance">
        <dropColumn columnName="DTYPE" tableName="USER_"/>
    </changeSet>
</databaseChangeLog>