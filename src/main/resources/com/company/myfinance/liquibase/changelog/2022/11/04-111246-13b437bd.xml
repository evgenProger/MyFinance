<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="myFinance">
        <addColumn tableName="TRANSACTION_">
            <column name="types_ID" type="UUID"/>
        </addColumn>
    </changeSet>
    <changeSet id="2" author="myFinance">
        <addForeignKeyConstraint baseColumnNames="types_ID" baseTableName="TRANSACTION_"
                                 constraintName="FK_TRANSACTION__ON_TYPES" referencedColumnNames="ID"
                                 referencedTableName="TYPE_"/>
    </changeSet>
</databaseChangeLog>